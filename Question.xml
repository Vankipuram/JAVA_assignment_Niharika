<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
  
  
   <bean id ="obj1" class = "com.cap.collections.Question">
    <property name = "questionId" value ="124"></property>
    <property name="question" value="What is your name?"> </property>
    <property name="answers1">
    <list>
    <value>John</value>
    <value>James</value>
    <value>Dennis</value>
    <value>Charles</value>
    </list>
    </property>
    </bean>
    
    
    <bean id ="obj2" class = "com.cap.collections.Question">
    <property name = "questionId" value ="124"></property>
    <property name="question" value="What is your favourite color?"> </property>
    <property name="answers2">
    <set>
    <value>"White"</value>
    <value>"Red"</value>
    <value>"Purple"</value>
    <value>"Orange"</value>
    </set>
    </property>
    </bean>
 
    
    
    <bean id ="obj3" class = "com.cap.collections.Question">
    <property name = "questionId" value ="124"></property>
    <property name="question" value="What is your favourite fruit?"> </property>
    <property name="answers3">
       <map>
         <entry key="1" value="Mango"></entry>
         <entry key="2" value="Apple"></entry>
          <entry key="3" value="Orange"></entry>
          <entry key="4" value="Strawberry"></entry>
        </map>
    </property>
    </bean>
    
    
  </beans>
   